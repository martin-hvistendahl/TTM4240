conf t
hostname Level-3

!set interfaces $type $name address $address
conf
set interfaces lo address 10.10.1.1/32
set interfaces eth0 address 10.10.0.3/29
    


set protocols bgp 3006 

set protocols bgp 3006 neighbor 10.10.0.2 remote-as 20001
set protocols bgp 3006 neighbor 10.10.0.2 description 'NextTel-2'

set protocols bgp 3006 neighbor 10.10.0.1 remote-as 4010
set protocols bgp 3006 neighbor 10.10.0.1 description 'TeleStar-2'

set protocols bgp 3006 address-family ipv4-unicast network 10.10.0.0/29
set protocols bgp 3006 address-family ipv4-unicast redistribute connected
set protocols static route 0.0.0.0/0 next-hop 0.0.0.0

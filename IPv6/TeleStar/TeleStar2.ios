conf t
ipv6 forwarding
int eth0
description ***SW UPLINK***
ipv6 address fd00:0:0:3000::1:1/112
bandwidth 1000

int eth1
description ***TELESTAR-1 UPLINK***
ipv6 address fd00:0:0:1000::2:2112
ipv6 ospf6 area 0
bandwidth 1000

int eth2
description ***TELESTAR-3 UPLINK***
ipv6 address fd00:0:0:1000::3:1/112
ipv6 ospf6 area 0
bandwidth 1000

int lo
description ***LOOPBACK***
ipv6 address fd00:0:0:4000::2/128


router ospf6
ospf6 router-id 2.2.2.2

end


